{
  "name": "@twilio-labs/match-components",
  "version": "0.2.0",
  "description": "Twilio Match Design System Components",
  "author": "Twilio Inc.",
  "license": "UNLICENSED",
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/twilio-labs/match",
    "directory": "packages/match-components"
  },
  "publishConfig": {
    "main": "dist/index.js",
    "module": "dist/index.es.js",
    "types": "dist/index.d.ts",
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf ./dist",
    "compile": "rollup -c node:match --environment NODE_ENV:production",
    "compile:dev": "rollup -c node:match --environment NODE_ENV:development",
    "type-check": "tsc --noEmit --composite false",
    "publish": "yarn npm publish --tolerate-republish"
  },
  "peerDependencies": {
    "@twilio-labs/match-themes": "^0.3.0",
    "@twilio-labs/match-tokens": "^0.3.0",
    "prop-types": "15.x",
    "react": "16.x",
    "react-dom": "16.x",
    "reakit": "1.x",
    "styled-components": "5.x"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@emotion/core": "10.0.35",
    "@storybook/react": "6.0.26",
    "@testing-library/react": "11.0.4",
    "@twilio-labs/match-themes": "workspace:packages/match-themes",
    "@twilio-labs/match-tokens": "workspace:packages/match-tokens/bundle",
    "@types/babel__core": "^7",
    "@types/jest-axe": "3.5.0",
    "@types/prop-types": "15.7.3",
    "@types/react": "16.9.51",
    "@types/react-dom": "16.9.8",
    "@types/styled-components": "5.1.3",
    "core-js": "3.6.5",
    "jest": "26.5.2",
    "jest-axe": "4.0.0",
    "jest-styled-components": "7.0.3",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-is": "16.13.1",
    "reakit": "1.2.5",
    "rollup": "2.28.2",
    "rollup-config-match": "workspace:configs/rollup",
    "styled-components": "5.2.0",
    "tsconfig-match": "workspace:configs/tsconfig",
    "tslib": "2.0.3",
    "typescript": "4.0.3"
  }
}
