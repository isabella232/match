{
  "name": "@twilio-labs/match-icons-twilio",
  "version": "0.0.0",
  "description": "Twilio Match Design System Icons",
  "author": "Twilio Inc.",
  "license": "UNLICENSED",
  "main": "src/index.tsx",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/twilio-labs/match",
    "directory": "packages/match-icons-twilio"
  },
  "publishConfig": {
    "main": "dist/index.js",
    "module": "dist/index.es.js",
    "types": "dist/index.d.ts",
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "convert:all": "rm -rf src && svgr -d src svg",
    "convert:new": "svgr -d src --ignore-existing svg",
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf ./dist",
    "compile": "rollup -c node:match --environment NODE_ENV:production",
    "compile:dev": "rollup -c node:match --environment NODE_ENV:development",
    "type-check": "tsc --noEmit --composite false",
    "publish": "yarn npm publish --tolerate-republish"
  },
  "peerDependencies": {
    "prop-types": "15.x",
    "react": "16.x",
    "react-dom": "16.x",
    "react-is": "16.x"
  },
  "devDependencies": {
    "@svgr/cli": "5.4.0",
    "@types/prop-types": "15.7.3",
    "@types/react": "16.9.51",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "rollup": "2.28.2",
    "rollup-config-match": "workspace:configs/rollup",
    "svgr-config-match": "workspace:configs/svgr",
    "tsconfig-match": "workspace:configs/tsconfig",
    "tslib": "2.0.3",
    "typescript": "4.0.3"
  }
}
