{
  "name": "@twilio-labs/match-themes",
  "version": "0.5.0",
  "description": "Twilio Match Design System Themes",
  "author": "Twilio Inc.",
  "license": "UNLICENSED",
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/twilio-labs/match",
    "directory": "packages/match-themes"
  },
  "publishConfig": {
    "main": "dist/index.js",
    "module": "dist/index.es.js",
    "types": "dist/index.d.ts",
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf ./dist",
    "compile": "rollup -c node:match --environment NODE_ENV:production",
    "compile:dev": "rollup -c node:match --environment NODE_ENV:development",
    "type-check": "tsc --noEmit --composite false",
    "publish": "yarn npm publish --tolerate-republish"
  },
  "peerDependencies": {
    "@twilio-labs/match-tokens": "^0.5.0",
    "prop-types": "15.x",
    "react": "17.x",
    "react-dom": "17.x",
    "react-is": "17.x",
    "styled-components": "5.x"
  },
  "devDependencies": {
    "@emotion/core": "11.0.0",
    "@testing-library/react": "11.2.2",
    "@twilio-labs/match-tokens": "workspace:packages/match-tokens/core",
    "@types/prop-types": "15.7.3",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/styled-components": "5.1.7",
    "core-js": "3.8.1",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-is": "17.0.1",
    "rollup": "2.35.1",
    "rollup-config-match": "workspace:configs/rollup",
    "styled-components": "5.2.1",
    "tsconfig-match": "workspace:configs/tsconfig",
    "tslib": "2.0.3",
    "typescript": "4.1.3"
  }
}
