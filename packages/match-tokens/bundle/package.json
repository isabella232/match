{
  "name": "@twilio-labs/match-tokens",
  "version": "0.3.0",
  "description": "A bundle containing all token libraries for use with the Twilio Match Design System.",
  "author": "Twilio Inc.",
  "license": "UNLICENSED",
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/twilio-labs/match",
    "directory": "packages/match-tokens/bundle"
  },
  "publishConfig": {
    "main": "dist/index.js",
    "module": "dist/index.es.js",
    "types": "dist/index.d.ts",
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf ./dist",
    "compile": "rollup -c node:match --environment NODE_ENV:production",
    "compile:dev": "rollup -c node:match --environment NODE_ENV:development",
    "type-check": "tsc --noEmit --composite false",
    "publish": "yarn npm publish --tolerate-republish"
  },
  "dependencies": {
    "@diez/prefabs": "10.6.0",
    "@twilio-labs/match-tokens-ahoy": "workspace:packages/match-tokens/ahoy",
    "@twilio-labs/match-tokens-sendgrid": "workspace:packages/match-tokens/sendgrid",
    "@twilio-labs/match-tokens-signal": "workspace:packages/match-tokens/signal",
    "@twilio-labs/match-tokens-twilio": "workspace:packages/match-tokens/twilio"
  },
  "devDependencies": {
    "core-js": "3.6.5",
    "rollup": "2.28.2",
    "rollup-config-match": "workspace:configs/rollup",
    "tsconfig-match": "workspace:configs/tsconfig",
    "tslib": "2.0.3",
    "typescript": "4.0.3"
  }
}
