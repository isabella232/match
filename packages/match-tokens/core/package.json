{
  "name": "@twilio-labs/match-tokens",
  "version": "1.0.0-alpha.1",
  "description": "Token libraries for the Twilio Match Design System.",
  "author": "Twilio Inc.",
  "license": "UNLICENSED",
  "main": "twilio/index.js",
  "module": "twilio/es6.js",
  "style": "twilio/variables.css",
  "types": "twilio/index.d.ts",
  "files": [
    "twilio",
    "sendgrid",
    "ahoy"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/twilio-labs/match",
    "directory": "packages/match-tokens/core"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf twilio sendgrid ahoy",
    "compile": "ts-node ./src/build.ts",
    "type-check": "tsc --noEmit --composite false",
    "publish": "yarn npm publish --tolerate-republish"
  },
  "devDependencies": {
    "@types/node": "14.14.25",
    "colortranslator": "1.3.5",
    "csstype": "3.0.6",
    "style-dictionary": "2.10.2",
    "ts-node": "9.1.1",
    "tsconfig-match": "workspace:configs/tsconfig",
    "typescript": "4.1.3"
  }
}
