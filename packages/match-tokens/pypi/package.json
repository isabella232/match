{
  "name": "match_tokens",
  "version": "0.0.1",
  "description": "Match token library for Python",
  "homepage": "https://match.twilio.design",
  "author": "Twilio Inc.",
  "license": "UNLICENSED",
  "private": true,
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/twilio-labs/match",
    "directory": "packages/match-tokens/pypi"
  },
  "scripts": {
    "build": "yarn clean && yarn compile",
