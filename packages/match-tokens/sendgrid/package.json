{
  "name": "@twilio-labs/match-tokens-sendgrid",
  "version": "0.1.0",
  "description": "SendGrid token library for use with the Twilio Match Design System.",
  "author": "Twilio Inc.",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "module": "dist/wrapper.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/twilio-labs/match",
    "directory": "packages/match-tokens/sendgrid"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "dependencies": {
    "@diez/web-sdk-common": "^10.6.0",
    "@twilio-labs/match-tokens-core": "workspace:packages/match-tokens/core"
  },
  "devDependencies": {
    "@diez/cli-core": "^10.6.0",
    "@diez/compiler-core": "^10.6.0",
    "@diez/engine": "^10.6.0",
    "@diez/prefabs": "^10.6.0",
    "@diez/stdlib": "^10.6.0",
    "@diez/targets": "^10.6.0",
    "core-js": "^3.6.5",
    "diez": "^10.6.0",
    "semver": "^7.3.2",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "bootstrap": "yarn build",
    "build": "yarn clean && yarn compile && mv ./build/diez-twilio-labs-match-tokens-sendgrid-web/ ./dist/ && rm -rf ./dist/package.json ./build ./lib ./tsconfig.tsbuildinfo",
    "compile": "diez compile -t web",
    "clean": "rm -rf ./dist ./.diez ./lib",
    "prepack": "yarn build",
    "publish": "yarn npm publish --tolerate-republish",
    "type-check": "tsc --noEmit --composite false"
  }
}
