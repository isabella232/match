{
  "name": "@twilio-labs/match-website",
  "version": "0.4.0",
  "private": true,
  "scripts": {
    "dev": "docusaurus start",
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf .docusaurus build",
    "compile": "docusaurus build",
    "serve": "docusaurus serve",
    "deploy": "gh-pages -d build -r https://${GH_TOKEN}@github.com/twilio-labs/match.git",
    "type-check": "tsc --noEmit --composite false"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-alpha.70",
    "@docusaurus/preset-classic": "2.0.0-alpha.70",
    "@docusaurus/remark-plugin-npm2yarn": "2.0.0-alpha.70",
    "@docusaurus/theme-classic": "2.0.0-alpha.70",
    "@docusaurus/theme-live-codeblock": "2.0.0-alpha.70",
    "@docusaurus/theme-search-algolia": "2.0.0-alpha.70",
    "@mdx-js/react": "1.6.22",
    "@styled-system/theme-get": "5.1.2",
    "@twilio-labs/match-components": "workspace:packages/match-components",
    "@twilio-labs/match-icons-product": "workspace:packages/match-icons/product",
    "@twilio-labs/match-icons-sendgrid": "workspace:packages/match-icons/sendgrid",
    "@twilio-labs/match-icons-twilio": "workspace:packages/match-icons/twilio",
    "@twilio-labs/match-primitives": "workspace:packages/match-primitives",
    "@twilio-labs/match-props": "workspace:packages/match-props",
    "@twilio-labs/match-themes": "workspace:packages/match-themes",
    "@twilio-labs/match-tokens": "workspace:packages/match-tokens/core",
    "clsx": "1.1.1",
    "colortranslator": "1.3.5",
    "docusaurus-plugin-react-docgen-typescript": "0.0.1",
    "gh-pages": "3.1.0",
    "plugin-docusaurus-tokens": "workspace:plugins/docusaurus-tokens",
    "pnp-webpack-plugin": "1.6.4",
    "react": "17.0.1",
    "react-docgen-typescript": "1.20.5",
    "react-dom": "17.0.1",
    "react-is": "17.0.1",
    "react-live": "2.2.3",
    "react-syntax-highlighter": "15.4.3",
    "react-uid": "2.3.1",
    "reakit": "1.3.4",
    "styled-components": "5.2.1",
    "styled-system": "5.1.5",
    "wcag-contrast": "3.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not IE <= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.0.0-alpha.70",
    "@types/gh-pages": "3.0.0",
    "@types/node": "14.14.20",
    "@types/prop-types": "15.7.3",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-is": "17.0.0",
    "@types/react-syntax-highlighter": "13.5.0",
    "@types/styled-components": "5.1.7",
    "@types/styled-system": "5.1.10",
    "prop-types": "15.7.2",
    "tsconfig-match": "workspace:configs/tsconfig",
    "typescript": "4.1.3"
  }
}
