{
  "name": "@twilio-labs/match-website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "docusaurus start",
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf .docusaurus build",
    "compile": "docusaurus build",
    "serve": "docusaurus serve",
    "deploy": "yarn build && gh-pages -d build -r https://${GH_TOKEN}@github.com/twilio-labs/match.git",
    "type-check": "tsc --noEmit --composite false"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-alpha.64",
    "@docusaurus/preset-classic": "2.0.0-alpha.64",
    "@docusaurus/theme-classic": "2.0.0-alpha.64",
    "@docusaurus/theme-live-codeblock": "2.0.0-alpha.64",
    "@mdx-js/react": "^1.6.18",
    "@twilio-labs/match-components": "workspace:packages/match-components",
    "@twilio-labs/match-fonts": "workspace:packages/match-fonts",
    "@twilio-labs/match-themes": "workspace:packages/match-themes",
    "@twilio-labs/match-tokens": "workspace:packages/match-tokens/bundle",
    "@twilio-labs/match-tokens-twilio": "workspace:packages/match-tokens/twilio",
    "clsx": "^1.1.1",
    "colortranslator": "^1.3.4",
    "docusaurus-plugin-react-docgen-typescript": "^0.0.1",
    "gh-pages": "^3.1.0",
    "pnp-webpack-plugin": "^1.6.4",
    "react": "^16.13.1",
    "react-docgen-typescript": "^1.20.5",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "styled-components": "^5.2.0",
    "wcag-contrast": "^3.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "v2.0.0-alpha.64",
    "@types/gh-pages": "^3.0.0",
    "@types/mdx-js__react": "^1.5.3",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-is": "^16",
    "@types/styled-components": "^5.1.3",
    "typescript": "^4.0.3"
  }
}
