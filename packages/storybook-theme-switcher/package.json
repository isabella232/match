{
  "name": "@twilio-labs/storybook-theme-switcher",
  "version": "0.0.0",
  "description": "Storybook addon to change the active Match theme",
  "private": true,
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/twilio-labs/match",
    "directory": "packages/match-themes"
  },
  "license": "UNLICENSED",
  "main": "dist/preview.js",
  "types": "dist/preview.d.ts",
  "files": [
    "dist",
    "*.js"
  ],
  "scripts": {
    "bootstrap": "yarn build",
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "compile": "tsc",
    "type-check": "tsc --noEmit --composite false"
  },
  "dependencies": {
    "@storybook/addons": "^6.0.21",
    "@storybook/api": "^6.0.21",
    "@storybook/components": "^6.0.21",
    "@storybook/theming": "^6.0.21",
    "@twilio-labs/match-themes": "workspace:packages/match-themes",
    "core-js": "^3.6.5"
  },
  "devDependencies": {
    "@types/node": "^14.6.4",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "tslib": "^2.0.1",
    "typescript": "^3.9.7"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  }
}
