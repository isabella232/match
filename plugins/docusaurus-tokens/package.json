{
  "name": "plugin-docusaurus-tokens",
  "version": "0.1.2",
  "description": "Registers Match tokens for use in Docusaurus themes",
  "private": true,
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/twilio-labs/match",
    "directory": "plugins/docusaurus-tokens"
  },
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "files": [
    "*.js"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "compile": "tsc",
    "type-check": "tsc --noEmit --composite false"
  },
  "dependencies": {
    "@twilio-labs/match-fonts": "workspace:packages/match-fonts",
    "@twilio-labs/match-tokens": "workspace:packages/match-tokens/core"
  },
  "devDependencies": {
    "@docusaurus/types": "2.0.0-alpha.70",
    "tsconfig-match": "workspace:configs/tsconfig",
    "typescript": "4.1.3"
  }
}
