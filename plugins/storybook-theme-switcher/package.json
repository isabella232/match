{
  "name": "plugin-storybook-theme-switcher",
  "version": "0.2.0",
  "description": "Storybook addon to change the active Match theme",
  "private": true,
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/twilio-labs/match",
    "directory": "plugins/storybook-theme-switcher"
  },
  "license": "UNLICENSED",
  "main": "dist/preview.js",
  "types": "dist/preview.d.ts",
  "files": [
    "dist",
    "*.js"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "compile": "tsc",
    "type-check": "tsc --noEmit --composite false"
  },
  "dependencies": {
    "@storybook/addons": "6.1.11",
    "@storybook/api": "6.1.11",
    "@storybook/components": "6.1.11",
    "@storybook/theming": "6.1.11",
    "@twilio-labs/match-themes": "workspace:packages/match-themes",
    "core-js": "3.8.1"
  },
  "devDependencies": {
    "@types/node": "14.14.14",
    "@types/prop-types": "15.7.3",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-is": "17.0.1",
    "tsconfig-match": "workspace:configs/tsconfig",
    "tslib": "2.0.3",
    "typescript": "4.1.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  }
}
